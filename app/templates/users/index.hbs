<div class="container users-container">

  <div class="row">
    <div class="col-md-8 col-md-offset-2">
      <div class="pull-right">
        <button {{action 'createNewUser'}} class="btn btn-primary">Ny användare</button>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-md-8 col-md-offset-2">
      <table class="table table-striped">
        <thead>
          <tr>
            <th>Användarnamn</th>
            <th>Roll</th>
          </tr>
        </thead>
        <tbody>
          {{#each model as |user|}}
            <tr>
              <td>{{user.username}}</td>
              <td>{{user.role}}</td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>
